<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel width="200px" height="100px">
		<g:VerticalPanel width="200px" height="100px" horizontalAlignment="ALIGN_CENTER">
			<g:Label text="New Label" ui:field="DBLabel" height="24px"/>
			<g:TextBox width="150px" ui:field="classNameInput"/>
			<g:HorizontalPanel>
				<g:Button ui:field="cancelButton">Cancel</g:Button>
				<g:SimplePanel width="6px" height="23px"/>
				<g:Button ui:field="confirmButton">Confirm</g:Button>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 