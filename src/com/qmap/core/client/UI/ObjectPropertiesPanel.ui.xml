<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel width="350" height="590" styleName="Outer-PropPanel">
		<g:CaptionPanel captionText="Object Properties" width="340px" height="590px">
			<g:VerticalPanel width="100%" height="100%">
			<g:Label text="" ui:field="subjectLabel"/>
			<g:Label text="" ui:field="objectLabel"/>
			<g:ScrollPanel ui:field="propertiesPanelScroller" height="480px" alwaysShowScrollBars="true">
				<g:VerticalPanel width="100%" height="100%" ui:field="propertiesVPanel"/>
			</g:ScrollPanel>
			<g:HorizontalPanel width="100%">
				<g:Button text="Create New Property..." ui:field="createNewProperty" enabled="false"/>
				<g:Button ui:field="createNewDataProperty" text="Create New Data Property..." enabled="false"/>
				<g:Button text="Confirm" ui:field="confirmBtn" height="40px" enabled="false"/>
			</g:HorizontalPanel>
		</g:VerticalPanel>
		</g:CaptionPanel>
	</g:HTMLPanel>
</ui:UiBinder> 